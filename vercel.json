{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "main.py"
    },
    {
      "src": "/organizations",
      "dest": "main.py"
    },
    {
      "src": "/filter-organizations",
      "dest": "main.py"
    },
    {
      "src": "^/$",
      "dest": "index.html"
    },
    {
      "src": "^/index.html$",
      "dest": "index.html"
    }
  ]
}
